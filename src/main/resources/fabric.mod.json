{
	"schemaVersion": 1,
	"id": "guardianexpansion",
	"version": "${version}",
	"name": "Guardian Expansion",
	"description": "Adds multiple items, equipment, and blocks themed around guardians!",
	"authors": [
		"ChickenSir"
	],
	"contact": {
		"homepage": "https://github.com/ChickenSir/guardian-expansion",
		"sources": "https://github.com/ChickenSir/guardian-expansion"
	},
	"license": "MIT",
	"icon": "assets/guardianexpansion/icon.png",
	"environment": "*",
	"entrypoints": {
		"fabric-datagen": [
			"com.guardianexpansion.datagen.GuardianExpansionDataGenerator"
		],
		"main": [
			"com.guardianexpansion.GuardianExpansion"
		],
		"client": [
			"com.guardianexpansion.GuardianExpansionClient"
		]
	},
	"mixins": [
		"guardianexpansion.mixins.json",
		{
			"config": "guardianexpansion.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}